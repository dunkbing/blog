{
  "name": "dunkbing-site",
  "author": "dunkbing",
  "repository": {
    "url": "https://github.com/dunkbing/dunkbing.github.io.git",
    "type": "git"
  },
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.3",
    "@tailwindcss/typography": "^0.4.1",
    "date-fns": "^2.29.2",
    "gray-matter": "^4.0.3",
    "mdx-prism": "^0.3.4",
    "next": "^12.2.5",
    "next-mdx-remote": "^4.1.0",
    "next-themes": "^0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-responsive-carousel": "^3.2.23",
    "react-tooltip": "^4.2.21",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "remark-code-titles": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^17.0.49",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.8",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^0.6.5",
    "lint-staged": "^11.2.6",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rehype": "^12.0.1",
    "tailwindcss": "^2.2.19",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.8.2"
  }
}