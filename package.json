{
  "name": "dunkbing-site",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dunkbing/dunkbing.github.io.git"
  },
  "license": "MIT",
  "author": "dunkbing",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "start": "next start",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "resolutions": {
    "@types/react": "18.2.6",
    "@types/react-dom": "17.0.2"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "mdx-prism": "^0.3.4",
    "next": "^13.4.1",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-responsive-carousel": "^3.2.23",
    "react-tooltip": "^4.5.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-code-titles": "^0.1.2",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.18.26",
    "@types/react": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^27.5.1",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^11.2.6",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "rehype": "^12.0.1",
    "tailwindcss": "^2.2.19",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.9.5"
  }
}